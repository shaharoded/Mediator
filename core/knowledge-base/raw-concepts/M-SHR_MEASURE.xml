<?xml version="1.0" encoding="UTF-8"?>
<calculated-raw-concept name="M-SHR_MEASURE" concept-type="raw-numeric">
    <categories>Measurements</categories>
    <description>Raw concept to manage the measurement of M-SHR ratio (glucose / first glucose measure)</description>
    <derived-from>
        <attribute name="GLUCOSE_MEASURE" tak="raw-concept" idx="0" ref="A1"/>
    </derived-from>
    
    <!-- Will use the closest instance to the beginning of the pattern -->
    <parameters>
        <parameter name="FIRST_GLUCOSE_MEASURE" tak="raw-concept" idx="0" ref="P1" default="72"/>
    </parameters>

    <function name="div">
        <value ref="A1"/>
        <parameter ref="P1"/>
    </function>
    
</calculated-raw-concept>