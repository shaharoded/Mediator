<?xml version="1.0" encoding="UTF-8"?>
<event name="RELEASE_EVENT">
    <categories>Events</categories>
    <description>Release event</description>
    <derived-from>
        <!-- ref optional when no abstraction-rules -->
        <attribute name="RELEASE" tak="raw-concept" idx="0" ref='A1'/>
    </derived-from>

    <abstraction-rules>
        <rule value="True" operator="or">
            <attribute ref="A1">
                <allowed-value equal="True"/>
            </attribute>
        </rule>
    </abstraction-rules>
</event>