<?xml version="1.0" encoding="UTF-8"?>
<event name="INFECTION_EVENT">
    <categories>Complications</categories>
    <description>Marks Infection (in admission complication) event</description>
    <derived-from>
        <!-- ref optional when no abstraction-rules -->
        <attribute name="INFECTION" tak="raw-concept" idx="0" ref='A1'/>
    </derived-from>

    <abstraction-rules>
        <rule value="True" operator="or">
            <attribute ref="A1">
                <allowed-value equal="True"/>
            </attribute>
        </rule>
    </abstraction-rules>
</event>