<?xml version="1.0" encoding="UTF-8"?>
<trend name="KETONES_URINE_MEASURE_TREND">
    <categories>Measurements</categories>
    <description>Trend for the measurements of KETONES_URINE</description>
    <derived-from name="KETONES_URINE_MEASURE" tak="raw-concept" idx="0" significant-variation="25"/>

    <!-- Each point will be classified as either "steady" or "changing" based on this window -->
    <time-steady value="6h"/>

    <!-- maximum difference between consecutive points to merge if both has the same classification -->
    <persistence good-after="12h"/>

</trend>